{#def invocations #}
<section id="table">
    <article style="padding:7px 0 0 0">
        <table class="striped">
            <thead>
                <tr>
                    {% set cols = ["ID", "TS (UTC)", "Credits", "Latency", "Success"] %}
                    {% for col in cols %}
                    <th>{{ col }}</th>
                    {% endfor %}
                </tr>
            </thead>
            <tbody>
                {% for invocation in invocations %}
                <InvocationRow invocation={invocation} />
                {% endfor %}
            </tbody>
        </table>
    </article>
</section>